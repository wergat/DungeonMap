local function get_v(id)
	if not fs.exists(".config") then
		return nil
	end
	local open = fs.open(".config", "r")
	local all = open.readAll()
	open.close()
	all = textutils.unserialize(all)
	if all[id] then
		return all[id]
	else
		return nil
	end
end
function set_factor()
	variableFactor = get_v("Command Spread Factor") or 5
	return variableFactor
end