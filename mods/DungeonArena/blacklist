local blacklist = {"TeamTwiistz"}

while true do
	for i = 1, #blacklist do
		local x = commands.exec("/testfor " .. blacklist[i])
		if x then
			commands.execAsync("/kill " .. blacklist[i])
			commands.execAsync('/tellraw @a ["",{"text":"Fuck you, ' .. blacklist[i] .. '.","color":"gold"}]')
		end
	end
	sleep(10)
end
