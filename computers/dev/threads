while not dcore do os.loadAPI(":dungeon/dcore/apis/dcore") sleep(1) end
print("< Running the threads, do not terminate >"
dcore.requireAPI("thread","cmd")
thread.add(dcore.checkForNewMessage,"DCORE Event Lister")
thread.add(cmd.chatchDoneCommandsLoop,"CMD Done Command Loop")
thread.add(cmd.stackCheckerLoop,"CMD Stack checker")
thread.run()