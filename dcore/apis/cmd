-- This API might get removed at some point, just wait for it...

function getAllPlayerNames(customSearch)
	local r,t = commands.xp(0,(customSearch or "@a"))
	local players = {}
	if(r)then
		for i=1,#t do
			players[#players+1] = string.match(t[i],"%a+$")
		end
		return players
	else
		print(t[1])
		return nil
	end
end